* 
  text-align:center
  font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif 
  font-weight: 300
  padding:0
  margin:0
  vertical-align:top
*:focus
  outline: none;
html,body
  height:100%
ul
  list-style-type: none
a
  text-decoration:none
pre
  white-space: pre-wrap
input, ul
  text-align:left
